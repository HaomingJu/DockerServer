############# config ##################
#
set base_path    /root
#
set mirror_path  $base_path/mirror
set skel_path    $base_path/skel
set var_path     $base_path/var
set cleanscript $var_path/clean.sh
#set defaultarch  <running host architecture>
set defaultarch  amd64
set postmirror_script $var_path/postmirror.sh
set run_postmirror 0
set nthreads     20
set _tilde 0
#
############# end config ##############

# Ubuntu18.04 LTS
deb-i386 http://mirrors.tuna.tsinghua.edu.cn/ubuntu bionic main restricted universe multiverse
deb-i386 http://mirrors.tuna.tsinghua.edu.cn/ubuntu bionic-updates main restricted universe multiverse
deb-i386 http://mirrors.tuna.tsinghua.edu.cn/ubuntu/ bionic-backports main restricted universe multiverse
deb-i386 http://mirrors.tuna.tsinghua.edu.cn/ubuntu bionic-security main restricted universe multiverse
deb-i386 http://mirrors.tuna.tsinghua.edu.cn/ros/ubuntu bionic main
deb-amd64 http://mirrors.tuna.tsinghua.edu.cn/ubuntu bionic main restricted universe multiverse
deb-amd64 http://mirrors.tuna.tsinghua.edu.cn/ubuntu bionic-updates main restricted universe multiverse
deb-amd64 http://mirrors.tuna.tsinghua.edu.cn/ubuntu bionic-backports main restricted universe multiverse
deb-amd64 http://mirrors.tuna.tsinghua.edu.cn/ubuntu bionic-security main restricted universe multiverse

